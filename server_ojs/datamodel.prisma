type Author @db(name: "authors") {
  author_id: Int! @id
  country: String
  email: String!
  firstName: String! @db(name: "first_name")
  includeInBrowse: Boolean! @db(name: "include_in_browse") @default(value: true)
  lastName: String! @db(name: "last_name")
  middleName: String @db(name: "middle_name")
  primaryContact: Boolean! @db(name: "primary_contact") @default(value: false)
  seq: Float! @default(value: 0)
  submissionId: Int! @db(name: "submission_id")
  suffix: String
  url: String
  userGroupId: Int @db(name: "user_group_id")
}

type ReviewAssignment @db(name: "review_assignments") {
  review_id: Int! @id
  competingInterests: String @db(name: "competing_interests")
  dateAcknowledged: DateTime @db(name: "date_acknowledged")
  dateAssigned: DateTime @db(name: "date_assigned")
  dateCompleted: DateTime @db(name: "date_completed")
  dateConfirmed: DateTime @db(name: "date_confirmed")
  dateDue: DateTime @db(name: "date_due")
  dateNotified: DateTime @db(name: "date_notified")
  dateRated: DateTime @db(name: "date_rated")
  dateReminded: DateTime @db(name: "date_reminded")
  dateResponseDue: DateTime @db(name: "date_response_due")
  declined: Boolean! @default(value: false)
  lastModified: DateTime @db(name: "last_modified")
  quality: Boolean
  recommendation: Boolean
  reminderWasAutomatic: Boolean! @db(name: "reminder_was_automatic") @default(value: false)
  replaced: Boolean! @default(value: false)
  reviewerFileId: Int @db(name: "reviewer_file_id")
  reviewerId: Int! @db(name: "reviewer_id")
  reviewFormId: Int @db(name: "review_form_id")
  reviewMethod: Boolean! @db(name: "review_method") @default(value: true)
  reviewRoundId: Int @db(name: "review_round_id")
  round: Boolean! @default(value: true)
  stageId: Boolean! @db(name: "stage_id") @default(value: true)
  step: Boolean! @default(value: true)
  submissionId: Int! @db(name: "submission_id")
  unconsidered: Boolean
}

type User @db(name: "users") {
  user_id: Int! @id
  authId: Int @db(name: "auth_id")
  authStr: String @db(name: "auth_str")
  billingAddress: String @db(name: "billing_address")
  country: String
  dateLastEmail: DateTime @db(name: "date_last_email")
  dateLastLogin: DateTime! @db(name: "date_last_login")
  dateRegistered: DateTime! @db(name: "date_registered")
  dateValidated: DateTime @db(name: "date_validated")
  disabled: Boolean! @default(value: false)
  disabledReason: String @db(name: "disabled_reason")
  email: String! @unique
  firstName: String! @db(name: "first_name")
  gender: String
  initials: String
  inlineHelp: Boolean @db(name: "inline_help")
  lastName: String! @db(name: "last_name")
  locales: String
  mailingAddress: String @db(name: "mailing_address")
  middleName: String @db(name: "middle_name")
  mustChangePassword: Boolean @db(name: "must_change_password")
  password: String!
  phone: String
  salutation: String
  suffix: String
  url: String
  username: String! @unique
}
